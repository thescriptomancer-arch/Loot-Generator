{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "entity.schema.json",
  "type": "object",
  "required": ["id", "names"],
  "properties": {
    "id": { "type": "string" },
    "type": { "type": "string" },
    "names": { "type": "array", "items": { "type": "string" } },
    "aliases": { "type": "array", "items": { "type": "string" } },
    "notes": { "type": "string" }
  }
}
